{
  "name": "insight-edu-backend",
  "version": "1.0.0",
  "description": "Backend API for Insight EDU - Cloudflare Workers",
  "main": "src/worker.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
